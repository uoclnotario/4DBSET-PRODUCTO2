            <Label alignment="CENTER" prefHeight="51.0" prefWidth="355.0" text="GESTIÓN DELEGACIONES">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="342.0" text="Nombre" />
