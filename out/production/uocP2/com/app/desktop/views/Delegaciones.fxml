            <Button fx:id="btnEliminar" mnemonicParsing="false" onAction="#borrar" prefHeight="51.0" prefWidth="346.0" text="Eliminar">
               <font>
                  <Font size="25.0" />
               </font></Button>
         </children>
      </VBox>
